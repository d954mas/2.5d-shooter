[project]
title = DTFJam
version = 0.1.0
dependencies = https://github.com/subsoap/defos/archive/master.zip,https://github.com/dapetcu21/defold-fmod/archive/v1.0.5.zip
custom_resources = assets/levels/result/
bundle_exclude_resources = 
bundle_resources = 

[bootstrap]
main_collection = /init.collectionc
render = /render/my.renderc

[input]
game_binding = /game.input_bindingc
use_accelerometer = 0

[display]
width = 1920
height = 1080
high_dpi = 1
fullscreen = 0
update_frequency = 60

[physics]
scale = 0.02
type = 3D
debug = 1

[script]
shared_state = 1

[library]
include_dirs = 

[native_extension]
app_manifest = /debug.appmanifest

[android]
package = com.d954mas.dtfjam
app_icon_36x36 = /assets/mobile/app_icons/icon_36.png
app_icon_48x48 = /assets/mobile/app_icons/icon_48.png
app_icon_72x72 = /assets/mobile/app_icons/icon_72.png
app_icon_96x96 = /assets/mobile/app_icons/icon_96.png
app_icon_144x144 = /assets/mobile/app_icons/icon_144.png
app_icon_192x192 = /assets/mobile/app_icons/icon_192.png
gcm_sender_id = 
push_field_text = 

[graphics]
texture_profiles = /default.texture_profiles
default_texture_min_filter = nearest
default_texture_mag_filter = nearest

[sound]
max_sound_buffers = 32

[sprite]
max_count = 1024

[resource]
max_resources = 1024

[collection]
max_instances = 512

[factory]
max_count = 128

[spine]
max_count = 1024

[ios]
app_icon_57x57 = /assets/mobile/app_icons/icon_57.png
